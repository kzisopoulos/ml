  "offline_enabled": true,
  "permissions": ["storage", "tabs"],
  "options_ui": {
    "page": "index.html",
    "chrome_style": true
  },
  "applications": {
    "gecko": {
      "id": "jimlkosmo7@gmail.com",
      "strict_min_version": "57.0"
    }
  }
}
